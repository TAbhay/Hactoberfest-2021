#include <bits/stdc++.h>
#include <cmath>
#include <cstdio>
#include <vector>
#include <algorithm>
using namespace std;
#define ll long long int
ll prime[1000001];
void sieve(int n)
{
    for(ll i=1;i<=n;i++) prime[i]=1;
    prime[0]=0;
    prime[1]=0;
    for(ll i=2;i*i<=n;i++)
    {
        if(prime[i])
        {
            for(ll j=i*i;j<=n;j+=i)
                prime[j]=0;
        }
    }
    for (int i=2; i<=n; i++) 
       if (prime[i]) 
          cout << i << " "; 
}
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t;
    cin>>t;
    while(t--)
    {
        ll n;
        cin>>n;
        sieve(n);
    }
    return 0;
}
